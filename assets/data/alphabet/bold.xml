<!DOCTYPE codename-engine-alphabet-font>
<alphabetFont fps="24" advance="50" lineGap="75" forceCasing="lower">
	<spritesheet>menus/alphabet-bold</spritesheet>

	<defaultAnim>character-LETTER0</defaultAnim>
	<anim char="&amp;">character-ampersand</anim>
	<anim char="&lt;">character-anglebracket-left</anim>
	<anim char="&gt;">character-anglebracket-right</anim>
	<anim char="*" y="30">character-asterisk</anim>
	<anim char="@">character-at</anim>
	<anim char="\">character-backslash</anim>
	<anim char="`" y="40">character-backtick</anim>
	<anim char="^" y="30">character-caret</anim>
	<anim char=":" y="10">character-colon</anim>
	<anim char="," y="-15">character-comma</anim>
	<anim char="{">character-curlybracket-left</anim>
	<anim char="}">character-curlybracket-right</anim>
	<anim char="$">character-dollar</anim>
	<anim char='"' y="35">character-doublequote0</anim>
	<anim char="=" y="20">character-equal</anim>
	<anim char="!">character-exclamationmark</anim>
	<anim char="-" y="25">character-hyphen</anim>
	<anim char="%">character-percent</anim>
	<anim char=".">character-period</anim>
	<anim char="+" y="10">character-plus</anim>
	<anim char="#">character-pound</anim>
	<anim char="?">character-questionmark0</anim>
	<anim char="(">character-roundbracket-left</anim>
	<anim char=")">character-roundbracket-right</anim>
	<anim char=";" y="-15">character-semicolon</anim>
	<anim char="'" y="35">character-singlequote</anim>
	<anim char="/">character-slash</anim>
	<anim char="[">character-squarebracket-left</anim>
	<anim char="]">character-squarebracket-right</anim>
	<anim char="~" y="20">character-tilde0</anim>
	<anim char="_">character-underscore0</anim>
	<anim char="|">character-verticalbar0</anim>

	<anim char="0">character-zero</anim>
	<anim char="1">character-one</anim>
	<anim char="2">character-two</anim>
	<anim char="3">character-three</anim>
	<anim char="4">character-four</anim>
	<anim char="5">character-five</anim>
	<anim char="6">character-six</anim>
	<anim char="7">character-seven</anim>
	<anim char="8">character-eight</anim>
	<anim char="9">character-nine</anim>
</alphabetFont>